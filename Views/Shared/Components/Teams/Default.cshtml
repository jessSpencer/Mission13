@{
}
@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" class="btn btn-outline-secondary">Home</a>

@foreach (string t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-bowlerTeam="@t"
       class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
}